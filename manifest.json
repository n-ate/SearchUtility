{
    "manifest_version": 2,
    "name": "WebSort Development",
    "short_name": "WebSort Dev",
    "description": "Allows creative search aggregation of multiple sites. Let me know what sites or searches you'd like supported.",
    "version": "0.0.0.4",
    "browser_action": {
        "default_icon": "shared/images/icon-128.png",
        "default_title": "WebSort Dev",
        "default_popup": "browser_action.html"
    },"icons": { 
        "16": "shared/images/icon-16.png",
        "48": "shared/images/icon-48.png",
       "128": "shared/images/icon-128.png" 
    },
    "permissions": [
        "webNavigation",
        "webRequest",
        "webRequestBlocking",
        "activeTab",
        "storage",
        "identity.email",
        "identity", 
        "tabs",
        "http://*/",
        "https://*/"
    ],
    "content_scripts": [
    ],
    "web_accessible_resources": [
        "shared/images/*.png",
        "shared/*.css",
        "admin/*.css",
        "shared/jquery.js",
        "shared/jquery-ui.js",
        "shared/tools/client-ui.js",
        "shared/tools/dom-fragment.js",
        "admin/tools/client-css.js",
        "admin/tools/dom-selector.js",
        "admin/play-wizard.js"
    ]
}